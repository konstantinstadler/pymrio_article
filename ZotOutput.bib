@article{mccollum2013_Climate,
  title = {Climate Policies Can Help Resolve Energy Security and Air Pollution Challenges},
  author = {McCollum, David L. and Krey, Volker and Riahi, Keywan and Kolp, Peter and Grubler, Arnulf and Makowski, Marek and Nakicenovic, Nebojsa},
