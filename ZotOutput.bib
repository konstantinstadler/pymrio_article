@article{pauliuk2015_General,
  title = {A {{General System Structure}} and {{Accounting Framework}} for {{Socioeconomic Metabolism}}},
  author = {Pauliuk, Stefan and Majeau-Bettez, Guillaume and M\"uller, Daniel B.},
