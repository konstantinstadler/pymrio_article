  author = {Giljum, Stefan and Behrens, Arno and Hinterberger, Friedrich and Lutz, Christian and Meyer, Bernd},
  date = {2008-05},
  journaltitle = {Environmental Science \& Policy},
  shortjournal = {Environmental Science \& Policy},
