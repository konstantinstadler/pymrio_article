@article{kounina2014_Spatial,
  title = {Spatial Analysis of Toxic Emissions in {{LCA}}: {{A}} Sub-Continental Nested {{USEtox}} Model with Freshwater Archetypes},
  shorttitle = {Spatial Analysis of Toxic Emissions in {{LCA}}},
