  keywords = {CGE model,Economic Policy,Economics/Management Science; general,Emissions intensity,Environmental Economics,Environmental Law/Policy/Ecojustice,Environmental Management,F18,Food miles debate,Livestock emissions,Q17,Q18,Q56,Q58,Transport emissions},
