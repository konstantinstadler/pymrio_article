@article{jackson2008_Where,
  title = {Where Is the ``Wellbeing Dividend''? {{Nature}}, Structure and Consumption Inequalities},
  shorttitle = {Where Is the ``Wellbeing Dividend''?},
  author = {Jackson, Tim},
  date = {2008-12-01},
  journaltitle = {Local Environment},
  volume = {13},
  pages = {703--723},
  issn = {1354-9839},
