  eprinttype = {pmid},
  file = {/home/konstans/zotbib/storage/5RSRQJMX/Kleijn et al - 2009 - On the relationship between farmland biodiversity and land-use intensity in.pdf},
  number = {1658},
  pmcid = {PMC2664376}
}

