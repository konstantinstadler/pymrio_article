@article{battefeld2012_Distinct,
  title = {Distinct Perinatal Features of the Hyperpolarization-Activated Non-Selective Cation Current {{I}} h in the Rat Cortical Plate},
  author = {Battefeld, Arne and Rocha, Nino and Stadler, Konstantin and Br\"auer, Anja U. and Strauss, Ulf},
  date = {2012-12-01},
  journaltitle = {Neural Development},
  shortjournal = {Neural Dev},
  volume = {7},
