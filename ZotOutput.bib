@article{liu2013_Divergent,
  title = {Divergent Global Precipitation Changes Induced by Natural versus Anthropogenic Forcing},
  author = {Liu, Jian and Wang, Bin and Cane, Mark A. and Yim, So-Young and Lee, June-Yi},
