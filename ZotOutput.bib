  shorttitle = {Beyond Peak Emission Transfers},
  author = {Wood, Richard and Grubb, Michael and Anger-Kraavi, Annela and Pollitt, Hector and Rizzo, Ben and Alexandri, Eva and Stadler, Konstantin and Moran, Dan and Hertwich, Edgar and Tukker, Arnold},
