@article{goktas2016_Remoteness,
  title = {Remoteness from Sources of Persistent Organic Pollutants in the Multi-Media Global Environment},
  author = {G\"okta\c{s}, Recep Kaya and MacLeod, Matthew},
  date = {2016-10},
  journaltitle = {Environmental Pollution},
  shortjournal = {Environmental Pollution},
