@article{steinberger2012_Pathways,
  title = {Pathways of Human Development and Carbon Emissions Embodied in Trade},
  author = {Steinberger, Julia K. and Timmons Roberts, J. and Peters, Glen P. and Baiocchi, Giovanni},
