@article{kleingoldewijk2011_HYDE,
  title = {The {{HYDE}} 3.1 Spatially Explicit Database of Human-Induced Global Land-Use Change over the Past 12,000 Years},
  author = {Klein Goldewijk, Kees and Beusen, Arthur and van Drecht, Gerard and de Vos, Martine},
